{
  "name": "cityhub",
  "version": "0.0.22",
  "license": "MIT",
  "author": {
    "name": "City Chain Foundation",
    "email": "citychainmail@gmail.com",
    "url": "https://citychain.foundation"
  },
  "description": "City Hub is your hub into everything on City Chain and the Smart City Platform.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityChainFoundation/city-hub.git"
  },
  "bugs": {
    "url": "https://github.com/CityChainFoundation/city-hub/issues"
  },
  "homepage": "https://github.com/CityChainFoundation/city-hub#readme",
  "main": "main.js",
  "scripts": {
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "start": "npm run postinstall:electron && npm run electron:tsc && npm-run-all -p ng:serve electron:serve",
    "start:prod": "npm run start -- -c production",
    "ng": "ng",
    "build": "npm run postinstall:electron && npm run electron:tsc && ng build",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:tsc": "tsc main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:testnet && electron .",
    "builder": "npm run build:prod && npx electron-builder build",
    "build:linux": "npm run builder -- --linux",
    "build:win:x86": "export arch=x86|| set arch=x86&& npm run builder -- --windows --ia32",
    "build:win:x64": "export arch=x64|| set arch=x64&& npm run builder -- --windows --x64",
    "build:mac": "npm run builder -- --mac",
    "build:all": "npm run builder -- -mwl",
    "publish": "npm run builder --  -wl -p always",
    "publish:mac": "npm run builder --  -m -p always",
    "publish:win:x86": "export arch=x86|| set arch=x86&& npm run builder --  -w --ia32 -p always",
    "publish:win:x64": "export arch=x64|| set arch=x64&& npm run builder --  -w  --x64 -p always",
    "publish:linux": "npm run builder --  -l -p always",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e"
  },
  "private": true,
  "dependencies": {
    "electron-context-menu": "^0.10.0",
    "electron-log": "^2.2.16",
    "electron-updater": "^3.0.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.1",
    "@angular/animations": "6.1.0",
    "@angular/cdk": "6.3.3",
    "@angular/cli": "6.1.1",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/compiler-cli": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/flex-layout": "6.0.0-beta.17",
    "@angular/forms": "6.1.0",
    "@angular/http": "6.1.0",
    "@angular/language-service": "6.1.0",
    "@angular/material": "6.3.3",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/router": "6.1.0",
    "@aspnet/signalr": "^1.0.2",
    "@types/bitcoinjs-lib": "^3.4.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "~8.9.4",
    "bip39": "^2.5.0",
    "bitcoinjs-lib": "^3.3.2",
    "codelyzer": "4.4.2",
    "core-js": "2.5.7",
    "electron": "^2.0.5",
    "electron-builder": "20.27.1",
    "electron-reload": "^1.2.5",
    "enhanced-resolve": "^4.1.0",
    "hammerjs": "2.0.8",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-clipboard": "^11.1.1",
    "ngx-electron": "^1.0.4",
    "ngx-qrcode2": "0.0.9",
    "npm-run-all": "^4.1.3",
    "npx": "^10.2.0",
    "protractor": "~5.3.0",
    "replace": "^1.0.0",
    "roboto-fontface": "^0.9.0",
    "rxjs": "6.2.2",
    "ts-node": "~5.0.1",
    "tslint": "5.10.0",
    "typescript": "~2.7.2",
    "wait-on": "^2.1.0",
    "zone.js": "0.8.26"
  }
}
