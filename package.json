{
  "name": "cityhub",
  "version": "1.0.27",
  "license": "MIT",
  "author": {
    "name": "City Chain Foundation",
    "email": "post@city-chain.org",
    "url": "https://www.city-chain.org"
  },
  "description": "City Hub is your hub into everything on City Chain and the Smart City Platform.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityChainFoundation/city-hub.git"
  },
  "bugs": {
    "url": "https://github.com/CityChainFoundation/city-hub/issues"
  },
  "homepage": "https://github.com/CityChainFoundation/city-hub#readme",
  "main": "main.js",
  "scripts": {
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "start": "npm run postinstall:electron && npm run electron:tsc && npm-run-all -p ng:lint ng:serve electron:serve",
    "start:debug": "npm run postinstall:electron && npm run electron:tsc && npm-run-all -p ng:lint ng:serve",
    "start:prod": "npm run start -- -c production",
    "ng": "ng",
    "build": "npm run postinstall:electron && npm run electron:tsc && ng lint && ng build",
    "build:prod": "npm run build -- -c production",
    "ng:lint": "ng lint",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:tsc": "tsc main.ts --lib DOM,ES2018",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:testnet && electron .",
    "builder": "npm run build:prod && npx electron-builder build",
    "build:linux": "npm run builder -- --linux",
    "build:linuxsnap": "npm run builder -- --linux snap",
    "build:win:x86": "export arch=x86|| set arch=x86&& npm run builder -- --windows --ia32",
    "build:win": "export arch=x64|| set arch=x64&& npm run builder -- --windows --x64",
    "build:osx": "npm run builder -- --mac",
    "build:all": "npm run builder -- -mwl",
    "publish": "npm run builder --  -wl -p always",
    "publish:osx": "npm run builder --  -m -p always",
    "publish:win:x86": "export arch=x86|| set arch=x86&& npm run builder --  -w --ia32 -p always",
    "publish:win": "export arch=x64|| set arch=x64&& npm run builder --  -w  --x64 -p always",
    "publish:linux": "npm run builder --  -l -p always",
    "publish:linuxsnap": "npm run builder --  -l snap -p always",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e",
    "version": "node -p \"require('./package.json').version\"",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "private": true,
  "dependencies": {
    "@msgpack/msgpack": "^1.12.1",
    "@techiediaries/ngx-qrcode": "^9.0.0",
    "bitcoinjs-message": "^2.1.0",
    "bs58": "^4.0.1",
    "conventional-changelog-cli": "2.0.31",
    "electron-context-menu": "0.16.0",
    "electron-log": "4.1.1",
    "electron-updater": "4.2.5",
    "ngx-image-cropper": "^3.1.5",
    "ngx-material-file-input": "^2.1.1",
    "read-chunk": "3.2.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular/animations": "10.0.2",
    "@angular/cdk": "9.2.0",
    "@angular/cli": "10.0.1",
    "@angular/common": "10.0.2",
    "@angular/compiler": "10.0.2",
    "@angular/compiler-cli": "10.0.2",
    "@angular/core": "10.0.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "10.0.2",
    "@angular/language-service": "10.0.2",
    "@angular/material": "9.2.0",
    "@angular/platform-browser": "10.0.2",
    "@angular/platform-browser-dynamic": "10.0.2",
    "@angular/router": "10.0.2",
    "@aspnet/signalr": "1.1.4",
    "@electron/typescript-definitions": "^8.7.3",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^12.11.1",
    "bip39": "3.0.2",
    "city-bip38": "3.0.1",
    "city-coininfo": "5.0.0",
    "city-lib": "5.0.0",
    "codelyzer": "^5.2.2",
    "core-js": "3.6.4",
    "dexie": "2.0.4",
    "electron": "8.0.0",
    "electron-builder": "22.4.1",
    "electron-reload": "1.5.0",
    "enhanced-resolve": "4.1.1",
    "hammerjs": "2.0.8",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-clipboard": "13.0.0",
    "ngx-electron": "2.2.0",
    "npm-run-all": "4.1.5",
    "npx": "10.2.2",
    "protractor": "~7.0.0",
    "replace": "1.1.5",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.5.5",
    "snyk": "^1.305.1",
    "ts-node": "8.5.0",
    "tslint": "~6.1.0",
    "typescript": "3.9.6",
    "wait-on": "4.0.2",
    "zone.js": "~0.10.3"
  },
  "snyk": true
}
