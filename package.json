{
  "name": "cityhub",
  "version": "1.0.20",
  "license": "MIT",
  "author": {
    "name": "City Chain Foundation",
    "email": "post@city-chain.org",
    "url": "https://citychain.foundation"
  },
  "description": "City Hub is your hub into everything on City Chain and the Smart City Platform.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityChainFoundation/city-hub.git"
  },
  "bugs": {
    "url": "https://github.com/CityChainFoundation/city-hub/issues"
  },
  "homepage": "https://github.com/CityChainFoundation/city-hub#readme",
  "main": "main.js",
  "scripts": {
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "start": "npm run postinstall:electron && npm run electron:tsc && npm-run-all -p ng:lint ng:serve electron:serve",
    "start:debug": "npm run postinstall:electron && npm run electron:tsc && npm-run-all -p ng:lint ng:serve",
    "start:prod": "npm run start -- -c production",
    "ng": "ng",
    "build": "npm run postinstall:electron && npm run electron:tsc && ng lint && ng build",
    "build:prod": "npm run build -- -c production",
    "ng:lint": "ng lint",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:tsc": "tsc main.ts --lib DOM,ES2018",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:testnet && electron .",
    "builder": "npm run build:prod && npx electron-builder build",
    "build:linux": "npm run builder -- --linux",
    "build:linuxsnap": "npm run builder -- --linux snap",
    "build:win:x86": "export arch=x86|| set arch=x86&& npm run builder -- --windows --ia32",
    "build:win:x64": "export arch=x64|| set arch=x64&& npm run builder -- --windows --x64",
    "build:mac": "npm run builder -- --mac",
    "build:all": "npm run builder -- -mwl",
    "publish": "npm run builder --  -wl -p always",
    "publish:mac": "npm run builder --  -m -p always",
    "publish:win:x86": "export arch=x86|| set arch=x86&& npm run builder --  -w --ia32 -p always",
    "publish:win:x64": "export arch=x64|| set arch=x64&& npm run builder --  -w  --x64 -p always",
    "publish:linux": "npm run builder --  -l -p always",
    "publish:linuxsnap": "npm run builder --  -l snap -p always",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "private": true,
  "dependencies": {
    "conventional-changelog-cli": "2.0.23",
    "electron-context-menu": "0.15.0",
    "electron-log": "3.0.9",
    "electron-updater": "4.1.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.19",
    "@angular/animations": "8.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/cli": "8.3.19",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@aspnet/signalr": "1.1.4",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.7.4",
    "bip39": "3.0.2",
    "city-bip38": "3.0.1",
    "city-coininfo": "5.0.0",
    "city-lib": "5.0.0",
    "dexie": "2.0.4",
    "codelyzer": "5.1.0",
    "core-js": "3.2.1",
    "electron": "6.0.7",
    "electron-builder": "21.2.0",
    "electron-reload": "1.5.0",
    "enhanced-resolve": "4.1.0",
    "hammerjs": "2.0.8",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "ngx-clipboard": "12.2.1",
    "ngx-electron": "2.1.1",
    "ngx-qrcode2": "0.0.9",
    "npm-run-all": "4.1.5",
    "npx": "10.2.0",
    "protractor": "5.4.2",
    "replace": "1.1.1",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.5.3",
    "ts-node": "8.5.0",
    "tslint": "5.20.1",
    "typescript": "3.5.3",
    "wait-on": "3.3.0",
    "zone.js": "0.10.2",
    "snyk": "^1.192.6"
  },
  "snyk": true
}
