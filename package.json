{
  "name": "city-hub",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityChainFoundation/city-hub.git"
  },
  "bugs": {
    "url": "https://github.com/CityChainFoundation/city-hub/issues"
  },
  "homepage": "https://github.com/CityChainFoundation/city-hub#readme",
  "main": "main.js",
  "scripts": {
    "postinstall": "npx electron-builder install-app-deps",
    "ng": "ng",
    "ng:serve": "ng serve",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build2": "node hooks/environments/set_profile.js && ng build && npm run electron:tsc",
    "mainnet": "node hooks/environments/set_profile.js && npm-run-all -p ng:serve electron:serve",
    "testnet": "node hooks/environments/set_profile.js && npm-run-all -p ng:serve electron:serve:test",
    "build:prod": "node hooks/environments/set_profile.js && ng build --prod && npm run electron:tsc",
    "electron:tsc": "tsc main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:serve:test": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve --testnet",
    "electron:local": "npm run build:prod && electron .",
    "package:linux": "npm run build:prod && npx electron-builder build --linux",
    "package:windows": "npm run build:prod && npx electron-builder build --windows",
    "package:mac": "npm run build:prod && npx electron-builder build --mac",
    "test2": "karma start ./karma.conf.js",
    "pree2e:build": "webdriver-manager update --gecko false && npm run ng:serve",
    "pree2e:protractor": "wait-on http-get://localhost:4200/ && protractor ./protractor.conf.js",
    "e2e2": "npm-run-all -p pree2e:build pree2e:protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.9",
    "@angular/cdk": "6.3.3",
    "@angular/common": "6.0.9",
    "@angular/compiler": "6.0.9",
    "@angular/core": "6.0.9",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "6.0.9",
    "@angular/http": "6.0.9",
    "@angular/material": "6.3.3",
    "@angular/platform-browser": "6.0.9",
    "@angular/platform-browser-dynamic": "6.0.9",
    "@angular/router": "6.0.9",
    "core-js": "2.5.7",
    "electron": "^2.0.5",
    "electron-builder": "^20.20.4",
    "electron-context-menu": "^0.10.0",
    "electron-reload": "^1.2.5",
    "hammerjs": "2.0.8",
    "ngx-clipboard": "^11.1.1",
    "ngx-electron": "^1.0.4",
    "ngx-qrcode2": "0.0.9",
    "roboto-fontface": "^0.9.0",
    "rxjs": "6.2.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.9",
    "@angular/language-service": "6.0.9",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "4.4.2",
    "enhanced-resolve": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.3",
    "npx": "^10.2.0",
    "protractor": "~5.3.0",
    "replace": "^1.0.0",
    "ts-node": "~5.0.1",
    "tslint": "5.10.0",
    "typescript": "~2.7.2",
    "wait-on": "^2.1.0"
  }
}
